<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jk.dao.SolrDao">
    <insert id="add" parameterType="com.jk.model.Product">
        insert into tb_item(id,title,sell_point,price,num)values (#{id},#{product_title},#{product_sell_point},#{product_price},#{product_num})
    </insert>
</mapper>